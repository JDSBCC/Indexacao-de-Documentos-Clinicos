@if (ViewBag.searchModel.Results != null)
{
    if (ViewBag.searchModel.Results.Count != 0)
    {
        foreach (var cont in ViewBag.searchModel.Results)
        {
            <div class='panel panel-default'>
                <div class='panel-body'>
                    <b>@cont["first_name"] @cont["last_name"] - @cont["dob"]</b><br/>
                    <small> @Html.Raw(cont["text"])</small>
                    <a href="@cont["documento_url"]">Documento</a>
                        - 
                    @Html.ActionLink("Informação Demográfica", "Metadata", "Home", new { id=cont["uid"]}, null)
                </div>
            </div>
        }
    }else{
        <h5>Nenhum resultado encontrado</h5>
    }
}
