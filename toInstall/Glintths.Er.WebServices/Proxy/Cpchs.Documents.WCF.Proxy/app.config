<?xml version="1.0"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="FilesEndpoint" />
                <binding name="LinksEndpoint" />
                <binding name="DocumentsEndpoint" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://gsousa.glintt.com/Cpchs.Documents.WCF.Host/Service.svc"
                binding="basicHttpBinding" bindingConfiguration="DocumentsEndpoint"
                contract="DocumentsManagementSR.DocumentsManagementSC" name="DocumentsEndpoint" />
            <endpoint address="http://gsousa.glintt.com/Cpchs.Documents.WCF.Host/FilesManagementWS.svc"
                binding="basicHttpBinding" bindingConfiguration="FilesEndpoint"
                contract="FilesManagementSR.FilesManagementSC" name="FilesEndpoint" />
            <endpoint address="http://gsousa.glintt.com/Cpchs.Documents.WCF.Host/LinksManagementWS.svc"
                binding="basicHttpBinding" bindingConfiguration="LinksEndpoint"
                contract="LinksManagementSR.LinksManagementSC" name="LinksEndpoint" />
        </client>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
